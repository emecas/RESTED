{
  "name": "rested",
  "title": "RESTED",
  "id": "rested@restedclient",
  "homepage": "https://github.com/esphen/RESTED",
  "description": "A REST client for the rest of us",
  "author": "Espen Henriksen",
  "license": "MIT",
  "version": "2.5.0",
  "main": "./main.js",
  "permissions": {
    "multiprocess": true
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchman --watchAll",
    "test:snapshot": "jest --update-snapshot",
    "test:debugger": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "eslint": "eslint src test",
    "eslint:fix": "eslint --fix src test",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack --watch",
	"build": "cross-env NODE_ENV=production webpack --no-cache",
    "precommit": "echo Please wait while tests run && npm run eslint && npm test"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redux": "^5.0.1",
    "react-redux": "^9.1.0",
    "@reduxjs/toolkit": "^2.2.5",
    "redux-saga": "^1.2.3",
    "styled-components": "^6.1.11",
	  "react-bootstrap": "^2.10.2",        
    "react-dnd": "^16.0.1",              
    "react-dnd-html5-backend": "^16.0.1",
    "classnames": "^2.5.1",
    "react-immutable-proptypes": "^2.2.0",
    "react-highlight": "^0.15.0",
    "immutable": "^4.3.6",
    "react-autosuggest": "^10.1.0",
    "fuzzysort": "^2.0.4",
    "uuid-js": "^0.7.5",
	  "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@swc/jest": "^0.2.36",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
